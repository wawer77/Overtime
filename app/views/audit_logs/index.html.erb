<h1>
  Audit Log
</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>
        #
      </th>
      <th>
        Employee
      </th>
      <th>
        Week starting
      </th>
      <th>
        Confirmed at
      </th>
      <th>
        Status
      </th>
    </tr>
  </thead>
  <tbody id='audit_logs'>
    <%= render @audit_logs %>
  </tbody>
</table>

<div id='paginator'>
  <%= paginate @audit_logs, remote: true %> 
</div>
